{
  "name": "FT Reader",
  "description": "Read the FT offline.",
  "version": "0.1",
  "app": {
    "background": {
      "scripts": [
      	"js/jquery-1.8.3.min.js",
      	"js/ft_reader.js",
      	"background.js"
      ]
    }
  },

  "permissions": [
  	"storage",
  	"alarms",
  	"tts",
  	"http://api.ft.com/*"
  ],

  "icons": { "16": "images/logo-32.png", "128": "images/logo-128.png" }
}